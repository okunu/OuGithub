<?xml version="1.0" encoding="utf-8"?>
<network-security-config>
    <domain-config cleartext-traffic-permitted="true">
        <domain includeSubdomains="true">api.github.com</domain>
        <trust-anchors>
            <certificates src="system" />
        </trust-anchors>
    </domain-config>
</network-security-config>
